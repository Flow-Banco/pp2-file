{
  "name": "@flow-fintech/pp2-file",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format:check": "biome format .",
    "format": "biome format --write .",
    "lint": "biome check ."
  },
  "keywords": [],
  "author": "flow-fintech",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.13.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flow-Banco/pp2-file.git"
  },
  "bugs": {
    "url": "https://github.com/Flow-Banco/pp2-file/issues"
  },
  "homepage": "https://github.com/Flow-Banco/pp2-file#readme",
  "description": ""
}
